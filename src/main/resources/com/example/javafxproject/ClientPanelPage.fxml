<AnchorPane fx:id="anch_Clientpage" prefHeight="555.0" prefWidth="768.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javafxproject.ClientPanelController">
