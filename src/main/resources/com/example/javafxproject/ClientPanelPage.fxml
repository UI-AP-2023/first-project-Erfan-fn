      </ImageView>
      <TitledPane fx:id="titPane_CostumerFacilities" prefHeight="182.0" prefWidth="202.0" text="Customer Facilities" AnchorPane.bottomAnchor="354.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="583.0" AnchorPane.topAnchor="0.0">
         <font>
            <Font name="Segoe UI Light" size="14.0" />
         </font>
         <content>
            <VBox fx:id="vBox_Facilities" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="150.0" prefWidth="200.0" spacing="10.0" style="-fx-background-color: #000000;">
               <children>
                  <Button fx:id="btn_personalInformation" mnemonicParsing="false" onAction="#showPersonalInformation" prefHeight="29.0" prefWidth="196.0" style="-fx-background-color: #054259;" text="Personal Information" textFill="WHITE">
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                     <font>
                        <Font name="Segoe UI Light" size="14.0" />
                     </font>
                  </Button>
